{
  "name": "v-error-boundary",
  "version": "0.0.4",
  "description": "error boundary component for vue3",
  "files": [
    "dist"
  ],
  "main": "./dist/v-error-boundary.umd.js",
  "module": "./dist/v-error-boundary.es.js",
