    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@types/node": "^17.0.42",
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.5.5",
    "vite": "^2.9.9",
    "vite-plugin-dts": "0.8.2",
    "vue-tsc": "^0.29.8"
  }
}
